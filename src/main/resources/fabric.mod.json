{
  "schemaVersion": 1,
  "id": "utfft",
  "version": "${version}",
  "name": "UTFFT",
  "description": "Use The Full F###ING Texture! Tell Minecraft to let resource packs use the full texture file for GUIs",
  "authors": ["Kirbbaebi"],
  "contact": {
    "repo": "https://github.com/Kirbbaebi/UTFFT/"
  },
  "license": "MIT",
  "icon": "assets/utfft/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "org.kirbbaebi.utfft.client.UTFFTClient"
    ],
    "main": [
      "org.kirbbaebi.utfft.UTFFT"
    ]
  },
  "mixins": [
    "utfft.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
